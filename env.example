# LangGraph GenUI 环境变量配置
# 复制此文件为 .env 并填入实际值

# LangSmith API 密钥（可选，用于追踪和调试）
LANGSMITH_API_KEY=your_langsmith_api_key_here

# Agent 服务配置
AGENT_PORT=8123
AGENT_HOST=0.0.0.0

# ChatUI 服务配置
CHATUI_PORT=3000
CHATUI_HOST=0.0.0.0

# 服务间通信配置
LANGGRAPH_API_URL=http://agent:8123
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NEXT_PUBLIC_ASSISTANT_ID=agent
